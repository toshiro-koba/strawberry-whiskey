<h1>タスクの新規登録</h1>

<span class="nav justify-content-end">
</span>

<%= form_with model: @ticket, local: true do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control', id: 'task_name' %>
  </div>
  <div class="form-group">
    <%= f.label :body %>
    <%= f.text_field :body, row: 3, class: 'form-control', id: 'task_body' %>
  </div>
  <%= f.submit nil, class: 'btn btn-primary' %>
<% end %>
